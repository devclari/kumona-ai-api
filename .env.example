# Configurações da aplicação
PORT=8080
LOG_LEVEL=INFO
ENVIRONMENT=development

# Google Cloud Platform
PROJECT_ID=seu-projeto-gcp
REGION=us-central1

# Configurações do modelo
MODEL_PATH=best_model.keras
MODEL_URL=https://drive.google.com/uc?id=1vSIfD3viT5JSxpG4asA8APCwK0JK9Dvu

# Configurações MLFlow
MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_EXPERIMENT_NAME=eye-disease-classifier
MLFLOW_MODEL_NAME=eye-disease-model
ENABLE_MLFLOW_TRACKING=true
ENABLE_MODEL_REGISTRY=false

# Configurações do Model Registry (se habilitado)
MODEL_STAGE=Production
MODEL_VERSION=latest

# Configurações de autenticação MLFlow (se necessário)
# MLFLOW_USERNAME=admin
# MLFLOW_PASSWORD=password

# Configurações de armazenamento MLFlow
MLFLOW_ARTIFACT_ROOT=./mlflow_artifacts

# Configurações de produção (para deploy)
# MLFLOW_TRACKING_URI=postgresql://user:pass@host:5432/mlflow
# MLFLOW_DEFAULT_ARTIFACT_ROOT=s3://my-bucket/mlflow-artifacts
